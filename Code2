#include <iostream>//Daxil edilen simvoldan ibaret kvadrat
using namespace std;

int main() {
    int a;
    char b;
    cout << "Kvadratin terefi: ";
    cin >> a;
    cout << "Simvol daxil edin: ";
    cin >> b;
    cout << endl;
    for(int i = 1; i <= a; i++){
        for(int k = 1; k <= a; k++){
            cout << b << b;
        }
        cout << endl;
    }
    return 0;
}
